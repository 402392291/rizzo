- pagination = YAML.load(File.read(File.expand_path('../../../data/styleguide/pagination_stubs.yml', __FILE__)))
.card.card--full-width.styleguide-intro
  .clearfix
    %h1.split--left.copy--title Pagination

- pagination.each_with_index do | stub, i |
  = sg_component('pagination', pagination: stub, count: i, full_width: true, card_style: true)