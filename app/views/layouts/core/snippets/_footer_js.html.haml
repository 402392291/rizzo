.js-config.is-hidden
  = render 'layouts/inline_js/fonts_async'
  = render 'layouts/core/snippets/fs_init'
  = render 'layouts/core/snippets/google_analytics' if Rails.env.production?
  = render 'layouts/inline_js/base_js_async'
  = yield :scripts

  = js_configuration
  - if defined?($JS_VARS)
    :javascript
      #{js_hash($JS_VARS)}

  - if is_lp_office?(true_client_ip(request))
    :javascript
      window.lpOffice = true;

