-# This is to support legacy apps link tracking in the subnav and breadcrumbs

%script
  jQuery('.row--secondary').on('click', '.nav__item--secondary', function() {
  window.s.linkstacker($(this).text() + "-sub");
  });

  jQuery('.row--breadcrumbs').on('click', '.nav__item--breadcrumbs', function(){
  window.s.linkstacker("breadcrumbs")
  });